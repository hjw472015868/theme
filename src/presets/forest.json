    "colors": {
      "none": "#2d4a2e",
      "default": "#2d4a2e",
      "card": "#3d5a40",
      "input": "#3d5a40",
      "thick": "#4caf50",
      "dashed": "#388e3c",
      "dotted": "#388e3c"
    }
