    "none": "none",
    "sm": "0 1px 3px 0 rgba(27, 94, 32, 0.3)",
    "md": "0 4px 8px -1px rgba(27, 94, 32, 0.4), 0 2px 4px -1px rgba(0, 0, 0, 0.3)",
    "lg": "0 10px 20px -3px rgba(27, 94, 32, 0.5), 0 4px 8px -2px rgba(0, 0, 0, 0.3)",
