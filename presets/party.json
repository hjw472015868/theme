    "colors": {
      "none": "#4a148c",
      "default": "#4a148c",
      "card": "#6a1b9a",
      "input": "#6a1b9a",
      "thick": "#e91e63",
      "dashed": "#ab47bc",
      "dotted": "#ce93d8"
    }
  },
