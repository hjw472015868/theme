    "style": {
      "solid": "solid",
      "dashed": "dashed",
      "dotted": "dotted"
    },
    "composite": {
      "none": "0",
      "default": "2px solid",
      "card": "2px solid",
      "input": "2px solid",
      "thick": "3px solid",
      "dashed": "2px dashed",
      "dotted": "2px dotted"
