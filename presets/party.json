    "none": "none",
    "sm": "0 0 10px 0 rgba(233, 30, 99, 0.3)",
    "md": "0 4px 20px -1px rgba(233, 30, 99, 0.5), 0 0 15px 0 rgba(3, 169, 244, 0.3)",
    "lg": "0 10px 30px -3px rgba(233, 30, 99, 0.6), 0 0 25px 0 rgba(3, 169, 244, 0.4)",
    "xl": "0 20px 40px -5px rgba(233, 30, 99, 0.7), 0 0 35px 5px rgba(3, 169, 244, 0.5)",
